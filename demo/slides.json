      {
        "type": "image",

        "image": "static/albums/range-rover/slogon.png",

        "styles": {
          "width": 150,
          "top": 50,
          "right": 20
        },

        "transition": {
          "timing": "in-back",

          "styles": {
            "enter": {
              "translateX": "-100px"
            },
            "stay": {
              "translateX": "0px"
            },
            "leave": {
              "translateX": "10px"
            }
          }
        }
      }
    ]
  },

  {
    "name": "BMW",
    "href": "http://www.bmw.com.cn/",
