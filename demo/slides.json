              "left": "0"
            }
          },
          {
            "type": "text",

            "text": "属于你的驾驶乐趣",

            "styles": {
              "width": "@containerWidth - 160",
              "color": "white",
              "font-size": "22px",
              "top": "0",
              "left": "160px"
            }
          }
        ],

        "styles": {
          "top": "10%",
          "left": "5%",
          "width": "100%",
          "height": "100%"
        },

        "transition": {
          "timing": "in-quart",

          "styles": {
            "enter": {
              "opacity": 0,
              "top": "0%"
            },
            "stay": {
              "opacity": 1,
              "top": "10%"
            },
            "leave": {
              "opacity": 0,
              "top": "0%"
            }
          }
        }
      }
    ]
  },

